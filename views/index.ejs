                        />
                      </div>

                      <div class="input-box">
                        <input
                          type="text"
                          id="name"
                          aria-describedby=""
                          placeholder="Full Name"
                          aria-required="true"
                          maxlength="30"
                          autocapitalize="off"
                          autocorrect="off"
                          name="name"
                          value=""
                          required
                        />
                      </div>

                      <div class="input-box">
                        <input
                          type="text"
                          id="name"
                          aria-describedby=""
                          placeholder="Username"
                          aria-required="true"
                          maxlength="30"
                          autocapitalize="off"
                          autocorrect="off"
                          name="username"
                          value=""
                          required
                        />
