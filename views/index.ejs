                        </svg><h3>Log in with Facebook</h3></a></button>
                  </div>
                  <div id="or">
                    <div id="l1"></div>
                    <div id="word"><h4>OR</h4></div>
                    <div id="l2"></div>
                  </div>
                  <!-- Header end -->
                  <div class="form-wrap">
                    <form action="/register" class="form" method="post">
                      <div class="input-box">
                        <input
                          type="text"
                          id="name"
                          aria-describedby=""
